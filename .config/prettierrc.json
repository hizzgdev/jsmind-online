{
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "singleAttributePerLine": false,
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "printWidth": 120
            }
        }
    ]
}
